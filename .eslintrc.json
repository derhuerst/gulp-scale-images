{
	"extends": "eslint:recommended",
	"env": {
		"es2018": true,
		"node": true
	},
	"ignorePatterns": [
		"node_modules"
	],
	"rules": {
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": false
            }
        ]
	}
}
